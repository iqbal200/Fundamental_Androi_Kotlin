<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".ActivityKetiga">

    <TextView
        android:id="@+id/tv_activity_ketiga"
        android:layout_width="251dp"
        android:layout_height="41dp"
        android:layout_gravity="center"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.131" />

    <Button
        android:id="@+id/btn_keempat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lanjut Keempat"
        app:layout_constraintBottom_toTopOf="@+id/tv_activity_ketiga"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        tools:layout_editor_absoluteX="0dp" />
    <Button
        android:id="@+id/btn_fragment1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fragment 1"
        app:layout_constraintBottom_toTopOf="@+id/tv_activity_ketiga"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        tools:layout_editor_absoluteX="0dp" />
    <Button
        android:id="@+id/btn_fagment2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fragment 2"
        app:layout_constraintBottom_toTopOf="@+id/tv_activity_ketiga"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        tools:layout_editor_absoluteX="0dp" />

    <fragment
        android:name="id.co.mka.fundamental_androi_kotlin.fragment.fragment_pertama"
        android:id="@+id/container_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />


</LinearLayout>